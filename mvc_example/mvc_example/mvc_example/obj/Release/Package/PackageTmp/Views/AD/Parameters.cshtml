@using mvc_example.Controllers
@{
    Layout = null;
    ADController.Parms parms = ViewBag.Parmss;
    HttpRequestBase rq = this.Request;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Parameters</title>
</head>
<body>
    <h3> parametry : @parms.X , @parms.Y </h3>
    <h3> parametry : @parms.s, @parms.n, @parms.d </h3>
    <h3>Request.Form =</h3>
    <p>
        @for (int i = 0; i < rq.Form.Count; i++)
        {
            <br />@rq.Form.GetKey(i)
            <span>= </span>
            @rq.Form.Get(rq.Form.GetKey(i))
        }
    </p>
    <form method="post">
        <p> string :<input name="string" type="text" /></p>
        <p> number :<input name="number" type="number" /></p>
        <p> date :<input name="date" type="date" /></p>
        <input type="submit" />
    </form>
</body>
</html>
